#include <stdio.h>
int main() {

   int a = 20;
   int b = 10;
   int c = 15;
   int d = 5;
   int e;

   e = (a + b) * c / d;      
   printf("Value of (%d + %d) * %d / %d is : %d\n", a, b, c, d, e );

   e = ((a + b) * c) / d;  
   printf("Value of ((%d + %d) * %d) / %d is  : %d\n" , a, b, c, d, e );

   e = (a + b) * (c / d);    
   printf("Value of (%d + %d) * (%d / %d) is  : %d\n", a, b, c, d, e );

   e = a + (b * c) / d;     
   printf("Value of %d + (%d * %d) / %d is  : %d\n" , a, b, c, d, e );

   return 0;
}
